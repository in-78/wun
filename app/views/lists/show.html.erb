<% content_for :lists do %>
  <%= render 'shared/list_table', lists: @lists %>
  <br />
  <%= render 'shared/create_lists' %>
  <%= link_to 'Destroy', @list, method: :delete, data: { confirm: 'Are you sure?' } %>
<% end %>

<% content_for :items do %>
  <%= render 'shared/create_items', list_id: @list.id %>
  <%= render 'shared/item_table', items: @items %>
<% end %>